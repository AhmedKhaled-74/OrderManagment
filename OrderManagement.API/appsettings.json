{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Default": "Data Source=(localdb)\\ProjectModels;Initial Catalog=OrderManagementDatabase;Integrated Security=True;Connect Timeout=30;Encrypt=False;Trust Server Certificate=False;Application Intent=ReadWrite;Multi Subnet Failover=False"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341"
        }
      }
    ]
  },
  "AllowOrigins": [
    "http://localhost:4200",
    "http://localhost:4201"
  ],
  "Jwt": {
    "Issuer": "https://localhost:7143",
    "Audience": "http://localhost:4200",
    "EXPIRATION_MINUTES": 1,
    "Refresh_EXPIRATION_MINUTES": 60,
    "Key" :  "My_Secret_Key_dont_tell_any_one_ahmedkhaledBasha" //no one will no it in production level only for devOp as env variable in development use and dummy key
  }
}
